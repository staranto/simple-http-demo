apiVersion: v1
kind: Service
metadata:
  labels:
    cdw.com/app: simple-http
    cdw.com/app.simple-http.role: counter-service
  name: counter-nodeport
  namespace: default
spec:
  ports:
  - name: "8181"
    port: 8181
    protocol: TCP
    targetPort: 8181
  selector:
    cdw.com/app.simple-http.role: counter
  sessionAffinity: None
  type: NodePort
status:
  loadBalancer: {}
